#src1Name=$($QUILT_HOME/bin/quilt_status | grep syslog | head -n 1 | awk '{print $1}' | sed  -e "s/{'//" -e "s/'://" -e "s/'//g")
fileName=$($QUILT_HOME/bin/quilt_status | sed -e "s/'//g")
